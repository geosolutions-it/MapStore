<?xml version="1.0" encoding="UTF-8"?>
<nurc:MD_Metadata xmlns:nurc="http://www.nurc.int/isoExtent" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dct="http://purl.org/dc/terms/" xmlns:ows="http://www.opengis.net/ows" xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:srv="http://www.isotc211.org/2005/srv" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gts="http://www.isotc211.org/2005/gts" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:wrs="http://www.opengis.net/cat/wrs/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.nurc.int/isoExtent ..\schema\NURC_Extent.xsd">
	<gmd:fileIdentifier>
		<gco:CharacterString>#UUID#</gco:CharacterString>
	</gmd:fileIdentifier>
	<gmd:language>
		<gmd:LanguageCode codeList="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/resources/Codelist/ML_gmxCodelists.xml#LanguageCode" codeListValue="eng">eng</gmd:LanguageCode>
	</gmd:language>
	<gmd:characterSet>
		<gmd:MD_CharacterSetCode codeList="resources/Codelist/gmxcodelists.xml#MD_CharacterSetCode" codeListValue="utf8"/>
	</gmd:characterSet>
	<gmd:hierarchyLevel>
		<gmd:MD_ScopeCode codeListValue="dataset" codeList="http://www.isotc211.org/2005/resources/codeList.xml#MD_ScopeCode"/>
	</gmd:hierarchyLevel>
	<!-- Metadatada conctact Information -->
	<gmd:contact/>
	<gmd:dateStamp>
		<gco:Date>#CREATION_DATE#</gco:Date>
	</gmd:dateStamp>
	<gmd:referenceSystemInfo>
		<gmd:MD_ReferenceSystem>
			<gmd:referenceSystemIdentifier>
				<gmd:RS_Identifier>
					<gmd:code>
						<gco:CharacterString>#SRS_CODE#</gco:CharacterString>
					</gmd:code>
					<gmd:codeSpace>
						<gco:CharacterString>#SRS_AUTORITY#</gco:CharacterString>
					</gmd:codeSpace>
				</gmd:RS_Identifier>
			</gmd:referenceSystemIdentifier>
		</gmd:MD_ReferenceSystem>
	</gmd:referenceSystemInfo>
	<gmd:identificationInfo>
		<gmd:MD_DataIdentification>
			<gmd:citation>
				<gmd:CI_Citation>
					<gmd:title>
						<!-- MAPPING NURC DATA : variable Name -->
						<gco:CharacterString>#VAR_DESCRIPTION#</gco:CharacterString>
					</gmd:title>
					<gmd:alternateTitle>
						<!-- MAPPING NURC DATA : Model Name-->
						<gco:CharacterString>#SATELLITE_NAME#-#SENSOR_NAME#</gco:CharacterString>
					</gmd:alternateTitle>
					<gmd:date>
						<!-- MAPPING NURC DATA : Metadata Creation Date -->
						<gmd:CI_Date>
							<gmd:date>
								<gco:Date>#CREATION_DATE#</gco:Date>
							</gmd:date>
							<gmd:dateType>
								<gmd:CI_DateTypeCode codeList="http://www.isotc211.org/2005/resources/codeList.xml#CI_DateTypeCode" codeListValue="creation"/>
							</gmd:dateType>
						</gmd:CI_Date>
					</gmd:date>
					<gmd:identifier>
						<!-- MAPPING NURC DATA : Compesed Identifier . <Metadata Type>:<Cruise>:<other Information>   -->
						<gmd:MD_Identifier>
							<gmd:code>
								<gco:CharacterString>remoteSensing:#VAR_NAME#:#SATELLITE_NAME#-#SENSOR_NAME#:#START_RANGE_TIME#:#END_RANGE_TIME#</gco:CharacterString>
							</gmd:code>
						</gmd:MD_Identifier>
					</gmd:identifier>
				</gmd:CI_Citation>
			</gmd:citation>
			<!-- MAPPING NURC DATA : Free Text Data description  -->
			<gmd:abstract>
				<gco:CharacterString>Free Text Data description</gco:CharacterString>
			</gmd:abstract>
			<gmd:pointOfContact>
				<gmd:CI_ResponsibleParty>
					<gmd:organisationName>
						<!-- MAPPING NURC DATA : Organization Data Originator-->
						<gco:CharacterString>NATO Undersea Research Centre (NURC)</gco:CharacterString>
					</gmd:organisationName>
					<gmd:role>
						<gmd:CI_RoleCode codeList="" codeListValue="originator"/>
					</gmd:role>
				</gmd:CI_ResponsibleParty>
			</gmd:pointOfContact>
			<gmd:pointOfContact>
				<gmd:CI_ResponsibleParty>
					<gmd:organisationName>
						<!-- MAPPING NURC DATA : Data Publisher-->
						<gco:CharacterString>Giampaolo Cimino</gco:CharacterString>
					</gmd:organisationName>
					<gmd:role>
						<gmd:CI_RoleCode codeList="" codeListValue="publisher"/>
					</gmd:role>
				</gmd:CI_ResponsibleParty>
			</gmd:pointOfContact>
			<gmd:pointOfContact>
				<gmd:CI_ResponsibleParty>
					<gmd:organisationName>
						<!-- MAPPING NURC DATA : Data Author-->
						<gco:CharacterString>Raffaele Grasso</gco:CharacterString>
					</gmd:organisationName>
					<gmd:role>
						<gmd:CI_RoleCode codeList="" codeListValue="author"/>
					</gmd:role>
				</gmd:CI_ResponsibleParty>
			</gmd:pointOfContact>
			<!-- Keyword Example-->
			<gmd:descriptiveKeywords>
				<gmd:MD_Keywords>
					<gmd:keyword>
						<gco:CharacterString>Remote Sensing</gco:CharacterString>
					</gmd:keyword>
					<gmd:type>
						<gmd:MD_KeywordTypeCode codeListValue="theme" codeList="http://www.isotc211.org/2005/resources/codeList.xml#MD_KeywordTypeCode"/>
					</gmd:type>
					<gmd:thesaurusName>
						<gmd:CI_Citation>
							<gmd:title>
								<gco:CharacterString>NURC themes, version 1.0</gco:CharacterString>
							</gmd:title>
							<gmd:date>
								<gmd:CI_Date>
									<gmd:date>
										<gco:Date>2009-06-01</gco:Date>
									</gmd:date>
									<gmd:dateType>
										<gmd:CI_DateTypeCode codeList="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/resources/Codelist/ML_gmxCodelists.xml#CI_DateTypeCode" codeListValue="publication">publication</gmd:CI_DateTypeCode>
									</gmd:dateType>
								</gmd:CI_Date>
							</gmd:date>
						</gmd:CI_Citation>
					</gmd:thesaurusName>
				</gmd:MD_Keywords>
			</gmd:descriptiveKeywords>
			<!-- MAPPING NURC DATA : Data Type -->
			<gmd:spatialRepresentationType>
				<gmd:MD_SpatialRepresentationTypeCode codeList="" codeListValue="grid"/>
			</gmd:spatialRepresentationType>
			<gmd:language>
				<gco:CharacterString>English</gco:CharacterString>
			</gmd:language>
			<gmd:extent>
				<gmd:EX_Extent>
					<gmd:geographicElement>
						<gmd:EX_GeographicDescription>
							<gmd:geographicIdentifier>
								<gmd:MD_Identifier>
									<gmd:code>
										<gco:CharacterString>it</gco:CharacterString>
									</gmd:code>
								</gmd:MD_Identifier>
							</gmd:geographicIdentifier>
						</gmd:EX_GeographicDescription>
					</gmd:geographicElement>
					<!-- MAPPING NURC DATA : Data Extent -->
					<gmd:geographicElement>
						<gmd:EX_GeographicBoundingBox>
							<gmd:westBoundLongitude>
								<gco:Decimal>#LONLATBBOX_MINX#</gco:Decimal>
							</gmd:westBoundLongitude>
							<gmd:eastBoundLongitude>
								<gco:Decimal>#LONLATBBOX_MAXX#</gco:Decimal>
							</gmd:eastBoundLongitude>
							<gmd:southBoundLatitude>
								<gco:Decimal>#LONLATBBOX_MINY#</gco:Decimal>
							</gmd:southBoundLatitude>
							<gmd:northBoundLatitude>
								<gco:Decimal>#LONLATBBOX_MAXY#</gco:Decimal>
							</gmd:northBoundLatitude>
						</gmd:EX_GeographicBoundingBox>
					</gmd:geographicElement>
					<gmd:temporalElement>
						<gmd:EX_TemporalExtent>
							<gmd:extent>
								<!-- MAPPING NURC DATA : Remote Sensing Data date/time Range -->
								<gml:TimePeriod gml:id="rangeDataTime">
									<gml:beginPosition>#START_RANGE_TIME#</gml:beginPosition>
									<gml:endPosition>#END_RANGE_TIME#</gml:endPosition>
								</gml:TimePeriod>
							</gmd:extent>
						</gmd:EX_TemporalExtent>
					</gmd:temporalElement>
				</gmd:EX_Extent>
			</gmd:extent>
		</gmd:MD_DataIdentification>
	</gmd:identificationInfo>

	<nurcExtent xmlns="http://www.nurc.int/isoExtent">
		<variablesList>
			<variable>
				<!-- Only one variable for the remote Sensing data-->
				<variableName>#VAR_NAME#</variableName>
				<variableUOM>#VAR_UOM#</variableUOM>
				<variableLongName>#VAR_DESCRIPTION#</variableLongName>
			</variable>
		</variablesList>
		<temporalInformation>
			<!-- Sorted (ascending order) Remote Sensing Time List. (Times are in milliseconds since 01/01/1970 and are separated by commas)-->
			<timeList>#TIME_LIST#</timeList>
		</temporalInformation>
		<dataInformation>
			<experimentOrCruise>#CRUISE_OR_EXP#</experimentOrCruise>
		</dataInformation>
		<distribuitionInfo>
			<owsLayer>
				<!-- OWS Layer Info-->
				<owsURL>#OWS_URL#</owsURL>
				<owsLayerName>#LAYER_NAME#</owsLayerName>
			</owsLayer>
		</distribuitionInfo>
		<dataSpecs>
			<remoteSensingData>
				<satellite>#SATELLITE_NAME#</satellite>
				<sensorName>#SENSOR_NAME#</sensorName>
				<postProcessingFlag>#POST_PROC_FLAG#</postProcessingFlag>
				<!-- if vectorValue= true-->
				<vectorDataInformation value="#VECT_FLAG#">
					<vectorDataType>#VECT_DATA_TYPE#</vectorDataType>
					<relatedData>#VECT_RELATED_DATA#</relatedData>
					<!-- polar (theta, ro) or cartesian (u, v)  -->
				</vectorDataInformation>
				<originalGridInformation>
					<referenceSystemIdentifier>#SRS_AUTORITY#:#SRS_CODE#</referenceSystemIdentifier>
					<pixelSize uom="#PIXEL_UOM#">
						<resX>#RESX#</resX>
						<resY>#RESY#</resY>
					</pixelSize>
					<gridSize>
						<width>#WIDTH#</width>
						<height>#HEIGHT#</height>
					</gridSize>
					<gridOrigin>#GRID_ORIGIN#</gridOrigin>
					<gridOffsets>#GRID_OFFSETS#</gridOffsets>
					<noDataValue>#NODATA#</noDataValue>
				</originalGridInformation>
			</remoteSensingData>
		</dataSpecs>
	</nurcExtent>
</nurc:MD_Metadata>
