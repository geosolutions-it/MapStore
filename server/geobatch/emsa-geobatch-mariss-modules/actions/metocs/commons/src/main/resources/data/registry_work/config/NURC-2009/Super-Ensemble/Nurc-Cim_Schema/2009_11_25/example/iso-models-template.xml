	<nurcExtent xmlns="http://www.nurc.int/isoExtent">
		<variablesList>
			<variable>
				<!-- Only one variable for the Model data-->
				<variableName>#VAR_NAME#</variableName>
				<variableUOM>#VAR_UOM#</variableUOM>
				<variableLongName>#VAR_DESCRIPTION#</variableLongName>
			</variable>
		</variablesList>
		<temporalInformation>
			<dataTime format="YYYY-MM-ddThh:mm:ss.sssZ">#MODEL_RUNTIME#</dataTime>
			<!-- Model Forecast Time-->
		</temporalInformation>
		<dataInformation>
			<experimentOrCruise>#CRUISE_OR_EXP#</experimentOrCruise>
		</dataInformation>
		<distribuitionInfo>
			<wcsLayer>
				<!-- Optional-->
				<wcsURL>#WCS_URL#</wcsURL>
				<wcsLayerName>#LAYER_NAME#</wcsLayerName>
			</wcsLayer>
			<wmsLayer>
				<!-- Optional-->
				<wmsURL>#WMS_URL#</wmsURL>
				<wmsLayerName>#LAYER_NAME#</wmsLayerName>
			</wmsLayer>
		</distribuitionInfo>
		<dataSpecs>
			<modelData>
				<modelName>#MODEL_NAME#</modelName>
				<modelTau uom="#TAU_UOM#">#MODEL_TAU#</modelTau>
				<modelRunTime format="YYYY-MM-ddThh:mm:ss.sssZ">#MODEL_RUNTIME#</modelRunTime>
				<modelType>#MODEL_TYPE#</modelType>
				<!-- Foracast or Analysis-->
				<postProcessingFlag>#POST_PROC_FLAG#</postProcessingFlag>
				<zLevel uom="#Z_UOM#" orientation="#Z_ORDER#">#Z_LEVEL#</zLevel>
				<!-- if vectorValue= true-->
				<vectorDataInformation value="#VECT_FLAG#">
					<vectorDataType>#VECT_DATA_TYPE#</vectorDataType>
					<relatedData>#VECT_RELATED_DATA#</relatedData>
					<!-- polar (theta, ro) or cartesian (u, v)  -->
				</vectorDataInformation>
				<originalGridInformation>
					<referenceSystemIdentifier>#SRS_AUTORITY#:#SRS_CODE#</referenceSystemIdentifier>
					<pixelSize uom="#PIXEL_UOM#">
						<resX>#RESX#</resX>
						<resY>#RESY#</resY>
					</pixelSize>
					<gridSize>
						<width>#WIDTH#</width>
						<height>#HEIGHT#</height>
					</gridSize>
					<gridOrigin>#GRID_ORIGIN#</gridOrigin>
					<gridOffsets>#GRID_OFFSETS#</gridOffsets>
					<noDataValue>#NODATA#</noDataValue>
				</originalGridInformation>
			</modelData>
		</dataSpecs>
	</nurcExtent>
