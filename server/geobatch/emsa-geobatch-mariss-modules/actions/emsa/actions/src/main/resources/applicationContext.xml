    	<constructor-arg type="String"><value>EGEOSGeoServerDeployerService</value></constructor-arg>
    	<constructor-arg type="String"><value>EGEOSGeoServerDeployerService</value></constructor-arg>
    </bean>


	<bean id="EGEOSAliasRegistrar" class="it.geosolutions.geobatch.egeos.deployers.actions.EGEOSAliasRegistrar" lazy-init="false">
        <constructor-arg ref="aliasRegistry" />
    </bean>
    
</beans>
